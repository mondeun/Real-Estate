@model IEnumerable<Real_Estate.Models.ZipCode>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ZipCodes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PricePerSqm)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ZipCodes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PricePerSqm)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ZipCodeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ZipCodeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ZipCodeID })
        </td>
    </tr>
}

</table>
